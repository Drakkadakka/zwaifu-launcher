{
  "security_settings": {
    "bind_host": "127.0.0.1",
    "max_connections": 10,
    "timeout_seconds": 30,
    "allowed_origins": [
      "http://127.0.0.1:5000",
      "http://localhost:5000"
    ],
    "enable_cors": true,
    "enable_rate_limiting": true,
    "max_requests_per_minute": 60
  },
  "path_validation": {
    "allowed_extensions": [
      ".bat",
      ".cmd",
      ".exe",
      ".py"
    ],
    "blocked_patterns": [
      "..",
      "~",
      "//",
      "\\\\",
      ":",
      "|",
      "*",
      "?",
      "<",
      ">",
      "\"",
      "'",
      "%",
      "&",
      ";",
      "`",
      "$",
      "(",
      ")",
      "{",
      "}",
      "[",
      "]"
    ],
    "max_path_length": 260
  },
  "port_validation": {
    "min_port": 1024,
    "max_port": 65535,
    "reserved_ports": [
      22,
      23,
      25,
      53,
      80,
      110,
      143,
      443,
      993,
      995
    ]
  }
}